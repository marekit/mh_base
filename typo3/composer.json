{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    }
  ],
  "require": {
    "typo3/cms": "~7.6.0",
    "dmitryd/typo3-realurl": "~2.0.0",
    "fluidtypo3/fluidcontent": "~4.4.0",
    "fluidtypo3/fluidpages": "~3.6.0",
    "fluidtypo3/flux": "~7.4.0",
    "fluidtypo3/vhs": "~3.0.0"
  },
  "require-dev": {},
  "scripts": {
    "post-install-cmd": "MH\\Base\\Composer\\InstallScript::runPostInstallCommand",
    "post-update-cmd": "MH\\Base\\Composer\\InstallScript::runPostUpdateCommand"
  },
  "config": {
    "vendor-dir": "/var/www/_share/vendor"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/../typo3/typo3_src-7",
      "web-dir": ""
    }
  },
  "autoload": {
    "psr-4": {
      "MH\\Base\\Composer\\": "../ext/mh_base/Classes/Composer",
      "MH\\Base\\": "typo3conf/ext/mh_base/Classes"
    }
  }
}
